{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}